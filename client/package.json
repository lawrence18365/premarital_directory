{
  "name": "premarital-counseling-directory",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^2.3.1",
    "@stripe/stripe-js": "^2.1.7",
    "@supabase/supabase-js": "^2.38.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "react-snap": "^1.23.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node scripts/generate-sitemap.js",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "build:sitemap": "node scripts/generate-sitemap.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "npm start",
    "build:prod": "npm run build",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'"
  },
  "reactSnap": {
    "source": "build",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "puppeteerExecutablePath": null,
    "inlineCss": true,
    "include": [
      "/",
      "/premarital-counseling",
      "/premarital-counseling/christian",
      "/premarital-counseling/catholic",
      "/premarital-counseling/lgbtq",
      "/premarital-counseling/online",
      "/premarital-counseling/gottman",
      "/premarital-counseling/prepare-enrich",
      "/premarital-counseling/interfaith",
      "/premarital-counseling/second-marriages",
      "/premarital-counseling/military",
      "/premarital-counseling/affordable",
      "/premarital-counseling/marriage-license-discount",
      "/premarital-counseling/california",
      "/premarital-counseling/texas",
      "/premarital-counseling/florida",
      "/premarital-counseling/new-york",
      "/premarital-counseling/georgia",
      "/premarital-counseling/north-carolina",
      "/premarital-counseling/illinois",
      "/premarital-counseling/pennsylvania",
      "/premarital-counseling/ohio",
      "/premarital-counseling/michigan",
      "/premarital-counseling/arizona",
      "/premarital-counseling/colorado",
      "/premarital-counseling/washington",
      "/premarital-counseling/tennessee",
      "/premarital-counseling/minnesota",
      "/about",
      "/features",
      "/blog",
      "/contact",
      "/professional/login",
      "/professional/signup"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
